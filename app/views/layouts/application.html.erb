<!DOCTYPE html>
<html>
<head>
  <title>Outpouring</title>
  <%= stylesheet_link_tag    "application", :media => "all" %>
  <%= javascript_include_tag "application" %>
  <%= csrf_meta_tags %>
</head>
<body>
  
	<%= render "shared/header" %>

  	<% flash.each do |name, msg| %>
    	<%= content_tag :div, msg, id: "flash_#{name}" %>
  	<% end %>
  	<div class="container main">
      <div class="logo">
      <%= image_tag 'logo3.png' %>
      </div>
  		  <%= yield %>
  	</div>
 	  


</body>
</html>
